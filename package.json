{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "description": "Simple starter package for Redux with React and Babel support",
  "main": "index.js",
  "repository": "git@github.com:CocoBeezy/ReactStarterKit.git",
  "scripts": {
    "start": "node server.dev.js",
    "build": "npm run clean && webpack --config webpack.config.prod.js --progress --colors",
    "deploy": "pm2 start server.js",
    "clean": "rimraf dist",
    "test": "jest --colors",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src --ext .js,.jsx",
    "pretty": "find src -name '*.js' -o -name '*.jsx' | xargs prettier --print-width 120 --single-quote --trailing-comma es5 --write",
    "precommit": "lint-staged"
  },
  "author": "Socorro Aguilar Jr.",
  "license": "ISC",
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "verbose": true,
    "setupTestFrameworkScriptFile": "<rootDir>/src/scripts/SetupTests.js",
    "moduleNameMapper": {
      "\\.scss$": "<rootDir>/src/scripts/SCSSStub.js"
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.4",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "browser-sync": "^2.12.8",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest-cli": "^22.4.4",
    "lint-staged": "^7.1.1",
    "node-sass": "^4.9.0",
    "pm2": "^2.10.4",
    "prettier": "^1.12.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.3.2",
    "redux-mock-store": "^1.5.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglify": "^0.1.5",
    "webpack": "^4.8.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-stream": "^3.2.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "immutability-helper": "^2.7.0",
    "keymirror": "^0.1.1",
    "lodash": "^4.17.10",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "styled-components": "^3.2.6"
  }
}
